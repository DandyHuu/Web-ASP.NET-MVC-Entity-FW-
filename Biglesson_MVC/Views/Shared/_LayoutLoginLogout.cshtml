    <!DOCTYPE HTML>
    <html>
    <head>
        <title>Login :: w3layouts</title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")
        <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
        <link rel="stylesheet" href="~/Content/AdminContent/bower_components/bootstrap/dist/css/bootstrap.min.css">
        <link href="~/Content/Login_css/css/bootstrap.css" rel="stylesheet" type="text/css" media="all">
        <!-- Custom Theme files -->
        <link href="~/Content/Login_css/css/style.css" rel="stylesheet" type="text/css" media="all" />
        <!--js-->
        <!--icons-css-->
        <link href="~/Content/Login_css/css/font-awesome.css" rel="stylesheet">
        <!--Google Fonts-->
        <link href='//fonts.googleapis.com/css?family=Carrois+Gothic' rel='stylesheet' type='text/css'>
        <link href='//fonts.googleapis.com/css?family=Work+Sans:400,500,600' rel='stylesheet' type='text/css'>
        <!--static chart-->
        <style>
            .forgot-grid ul li input[type="checkbox"]:checked + label span:first-child:before {
                content: "";
                background: url(../../Content/images/tick.png)no-repeat 1px -1px;
                position: absolute;
                left: 1px;
                top: 2px;
                font-size: 10px;
                width: 10px;
                height: 10px;
            }
        </style>
    </head>
    <body>
        <main>
            @RenderBody()
        </main>
        <div class="copyrights">
            <p>All rights reserved | This template is made with © by <a href="https://www.facebook.com/CHiyiSyis" target="_blank">Dandy Hữu</a> </p>
        </div>
        <!--COPY rights end here-->
        <!-- REQUIRED JS SCRIPTS -->
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)

        <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>

        <!--scrolling js-->
        <script src="@Url.Content("~/Scripts/Login_js/js/jquery.nicescroll.js")"></script>
        <script src="@Url.Content("~/Scripts/Login_js/js/scripts.js")"></script>
        <!--//scrolling js-->
        <script src="@Url.Content("~/Scripts/Login_js/js/bootstrap.js")"></script>
        <!-- mother grid end here-->
        <script src="@Url.Content("~/Scripts/validator.js")"></script>
    </body>
</html>

</body>
</html>
